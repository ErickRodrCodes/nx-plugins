{
  "name": "electron-angular-demo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/electron-angular-demo/src",
  "projectType": "application",
  "tags": [],
  "implicitDependencies": ["demo-angular"],
  "targets": {
    "electron": {
      "configurations": {
        "build": {
          "dependsOn": ["^build"],
          "options": {
            "commands": [
              "nx run electron-angular-demo:build"
            ],
            "parallel": false
          }
        },
        "serve": {
          "commands": [
            "nx run demo-angular:serve",
            "nx run electron-angular-demo:serve"
          ],
          "parallel": true
        }
      },
      "executor": "nx:run-commands",
      "defaultConfiguration": "serve"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "dependsOn": ["^test"],
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/electron-angular-demo/jest.config.ts"
      }
    },
    "lint": {
      "dependsOn": ["^lint"]
    }
  }
}
