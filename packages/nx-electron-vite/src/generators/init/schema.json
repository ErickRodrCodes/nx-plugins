{
  "$schema": "https://json-schema.org/schema",
  "$id": "Init",
  "title": "Init @erickrodrcodes/nx-electron-vite",
  "type": "object",
  "properties": {
    "skipPackageJson": {
      "description": "Do not add dependencies to `package.json`.",
      "type": "boolean",
      "default": false
    },
    "skipFormat": {
      "description": "Skip formatting files.",
      "type": "boolean",
      "default": false
    },
    "skipInstallPluginDependencies": {
      "description": "Do not install Plugin dependencies with your package manager.",
      "type": "boolean",
      "default": false
    }
  },
  "required": []
}
