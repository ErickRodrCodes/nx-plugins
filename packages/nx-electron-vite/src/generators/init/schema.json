{
  "$schema": "https://json-schema.org/schema",
  "$id": "Init",
  "title": "Init @erickrodrcodes/nx-electron-vite",
  "type": "object",
  "properties": {
    "skipPackageJson": {
      "description": "Do not add dependencies to `package.json`.",
      "type": "boolean",
      "default": false
    },
    "skipFormat": {
      "description": "Skip formatting files.",
      "type": "boolean",
      "default": false
    },
    "installPluginDependencies": {
      "description": "Installing Plugin dependencies. If you set this to false you might need to run your package manager install command to complete the install.",
      "type": "boolean",
      "default": true
    }
  },
  "required": []
}
