{
  "name": "nx-electron-vite",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "package/nx-electron-vite/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/package/nx-electron-vite",
        "main": "package/nx-electron-vite/src/index.ts",
        "tsConfig": "package/nx-electron-vite/tsconfig.lib.json",
        "assets": [
          "package/nx-electron-vite/*.md",
          {
            "input": "./package/nx-electron-vite/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./package/nx-electron-vite/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./package/nx-electron-vite",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./package/nx-electron-vite",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "package/nx-electron-vite/jest.config.ts"
      }
    }
  }
}
